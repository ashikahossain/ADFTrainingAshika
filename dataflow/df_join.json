{
	"name": "df_join",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Movies2",
						"type": "DatasetReference"
					},
					"name": "Movies",
					"description": "Import/updating\n data from Movies2"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "MoviesClean"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Movies",
				"Movies derive(column1 = title1,",
				"          column2 = ERROR_FUNCTION('')) ~> MoviesClean"
			]
		}
	}
}